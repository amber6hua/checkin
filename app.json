{
  "name": "Heroku-autocheckin",
  "description": "autocheckin",
  "keywords": ["autocheckin"],
  "website": "https://github.com/amber6hua/autocheckin",
  "repository": "https://github.com/amber6hua/autocheckin",
  "env": {
    "API_ID": {
      "description": "API ID of Telegram."
    },
    "API_HASH": {
      "description": "API HASH of Telegram."
    },
    "DATABASE_URL": {
      "description": "postgresql DATABASE_URL.",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev"
    },
    {
      "plan": "newrelic:wayne"
    }
  ]
}
